{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "title": "SpaceX Craft",
    "apiVersion": 2,
    "name": "spacex/craft",
    "category": "text",
    "description": "A simple block to show spacex capsules as grid.",
    "textdomain": "spacex-craft",
    "keywords": ["spacex", "rocket", "capsule"],
    "supports": {
        "align": ["wide", "left", "right", "center", "full"],
        "spacing": {
            "margin": true,
            "padding": true,
            "__experimentalDefaultControls": {
                "padding": true
            }
        },
        "__experimentalBorder": {
            "color": true,
            "radius": true,
            "style": true,
            "width": true
        }
    },
    "attributes": {
        "search_by": {
            "type": "string",
            "default": ""
        }
    },
    "editorScript": "spx-block-script"
}
  